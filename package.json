{
  "name": "cardoctor-comet-chat",
  "version": "1.0.20",
  "description": "cardoctor cometchat npm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Tuan Nguyen",
  "keywords": ["cardoctor","cometchat","comet chat","comet"],
