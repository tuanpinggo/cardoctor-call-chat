    "react": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@cometchat/calls-sdk-javascript": "^4.1.0",
    "@cometchat/chat-uikit-react": "^6.2.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "@cometchat/chat-uikit-react": ">=4.1.0",
    "@cometchat/chat-sdk-javascript": ">=6.2.0"
  },
  "bugs": {
    "url": "https://github.com/tuanpinggo/cardoctor-call-chat/issues"
